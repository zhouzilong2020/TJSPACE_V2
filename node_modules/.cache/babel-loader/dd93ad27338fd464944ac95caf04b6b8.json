le.log(\"handle route change\", payload);\n      this.$router.push({\n        name: payload,\n        params: {\n          userId: payload.userid,\n        },\n      });\n    },\n    async handleLogout() {\n      console.log(\"clicking logout btn\");\n      await this.$store.dispatch(\"userInfo/logoutUser\");\n      // 成功退出\n      if (this.token == null) {\n        console.log(\"logout user successfully\");\n        this.$router.push({\n          name: \"index\",\n        });\n      }\n    },\n  },\n};\n"]}]}